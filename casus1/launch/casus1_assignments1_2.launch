<?xml version="1.0"?>
<launch>
  <!-- Start the sensor info publisher node.-->
  <node name="sensor_info" pkg="casus1" type="sensor_info_publisher.py"
    output="screen"/>

  <!-- Start the BoxHeightInformation publisher node (Assignment 1)-->
  <node name="box_height_metres" pkg="casus1" type="assignment1.py"
    output="screen"/>

  <!-- Start the metres_to_feet service server ROS node.-->
  <node name="metres_to_feet" pkg="casus1" type="metres_to_feet_server.py"
    output="screen"/>

  <!-- BoxHeightInformation subscriber / convert to Feet node (Assignment 2)-->
  <node name="box_height_feet" pkg="casus1" type="assignment2.py"
    output="screen"/>
</launch>
